    "@types/node": "^20.14.10",
    "@types/prompts": "^2.4.9",
    "@types/seedrandom": "^3.0.5",
	  "@vue/eslint-config-typescript": "^14.6.0",
    "eslint": "^9.17.0",
    "eslint-plugin-format": "^0.1.3",
	  "eslint-plugin-vue": "^10.5.1",
    "esno": "^4.8.0",
    "execa": "^9.5.2",
    "fast-glob": "^3.3.2",
    "playwright-chromium": "^1.45.1",
    "pnpm": "^9.5.0",
    "prettier-plugin-slidev": "^1.0.5",
    "prompts": "^2.4.2",
    "seedrandom": "^3.0.5",
    "typescript": "^5.5.3",
