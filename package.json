  "private": true,
  "packageManager": "pnpm@9.5.0",
  "scripts": {
    "dev": "pnpm run picker dev --open",
    "build": "rm -rf dist && pnpm run picker build",
    "build:all": "rm -rf dist && pnpm -r run build",
    "export": "pnpm run picker export",
    "typecheck": "vue-tsc --noEmit",
