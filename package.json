    "picker": "esno scripts/picker.ts",
    "prepare": "git submodule update --init --recursive"
  },
  "devDependencies": {
    "@iconify/json": "^2.2.226",
    "@slidev/cli": "^0.49.16",
    "@slidev/client": "^0.49.16",
